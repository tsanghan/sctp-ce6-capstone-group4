        {
            "type": "metric",
            "x": 6,
            "y": 12,
            "width": 6,
            "height": 6,
            "properties": {
                "region": "${region}",
                "title": "Pod status",
                "legend": {
                    "position": "bottom"
                },
                "timezone": "LOCAL",
                "metrics": [
                    [ { "expression": "IF(PERIOD(m1m0)>=60, m1m0/(PERIOD(m1m0)/60), m1m0)", "label": "pod_status_initialized", "accountId": "${account_id}" } ],
                    [ { "expression": "IF(PERIOD(m2m0)>=60, m2m0/(PERIOD(m2m0)/60), m2m0)", "label": "pod_status_pending", "accountId": "${account_id}" } ],
                    [ { "expression": "IF(PERIOD(m3m0)>=60, m3m0/(PERIOD(m3m0)/60), m3m0)", "label": "pod_status_succeeded", "accountId": "${account_id}" } ],
                    [ { "expression": "IF(PERIOD(m4m0)>=60, m4m0/(PERIOD(m4m0)/60), m4m0)", "label": "pod_status_failed", "accountId": "${account_id}" } ],
                    [ { "expression": "IF(PERIOD(m5m0)>=60, m5m0/(PERIOD(m5m0)/60), m5m0)", "label": "pod_status_running", "accountId": "${account_id}" } ],
                    [ { "expression": "IF(PERIOD(m6m0)>=60, m6m0/(PERIOD(m6m0)/60), m6m0)", "label": "pod_status_ready", "accountId": "${account_id}" } ],
                    [ { "expression": "IF(PERIOD(m7m0)>=60, m7m0/(PERIOD(m7m0)/60), m7m0)", "label": "pod_status_scheduled", "accountId": "${account_id}" } ],
                    [ { "expression": "IF(PERIOD(m8m0)>=60, m8m0/(PERIOD(m8m0)/60), m8m0)", "label": "pod_status_unknown", "accountId": "${account_id}" } ],
                    [ "ContainerInsights", "pod_status_initialized", "ClusterName", "tsanghan-ce6", { "id": "m1m0", "stat": "Sum", "visible": false, "accountId": "${account_id}" } ],
                    [ ".", "pod_status_pending", ".", ".", { "id": "m2m0", "stat": "Sum", "visible": false, "accountId": "${account_id}" } ],
                    [ ".", "pod_status_succeeded", ".", ".", { "id": "m3m0", "stat": "Sum", "visible": false, "accountId": "${account_id}" } ],
                    [ ".", "pod_status_failed", ".", ".", { "id": "m4m0", "stat": "Sum", "visible": false, "accountId": "${account_id}" } ],
                    [ ".", "pod_status_running", ".", ".", { "id": "m5m0", "stat": "Sum", "visible": false, "accountId": "${account_id}" } ],
                    [ ".", "pod_status_ready", ".", ".", { "id": "m6m0", "stat": "Sum", "visible": false, "accountId": "${account_id}" } ],
                    [ ".", "pod_status_scheduled", ".", ".", { "id": "m7m0", "stat": "Sum", "visible": false, "accountId": "${account_id}" } ],
                    [ ".", "pod_status_unknown", ".", ".", { "id": "m8m0", "stat": "Sum", "visible": false, "accountId": "${account_id}" } ]
                ],
                "liveData": false,
                "period": 60,
                "yAxis": {
                    "left": {
                        "label": "Count",
                        "showUnits": false
                    }
                }
            }
        },
