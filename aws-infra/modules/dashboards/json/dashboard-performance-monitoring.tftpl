            "width": 3,
            "height": 6,
            "properties": {
                "region": "${region}",
                "title": "Nodes status",
                "legend": {
                    "position": "right"
                },
                "timezone": "LOCAL",
                "metrics": [
                    [ { "id": "ready_nodes_avgm0", "label": "Ready Nodes (avg)", "color": "#2ca02c", "expression": "IF(PERIOD(ready_nodesm0)>=60, ready_nodesm0/(PERIOD(ready_nodesm0)/60), ready_nodesm0)", "accountId": "${account_id}" } ],
                    [ { "id": "m0m0", "label": "Not Ready Nodes (avg)", "color": "#ff7f0e", "expression": "IF(total_nodes_avgm0-ready_nodes_avgm0 >=0, total_nodes_avgm0-ready_nodes_avgm0, 0)", "accountId": "${account_id}" } ],
                    [ { "id": "total_nodes_avgm0", "color": "#ff7f0e", "expression": "IF(PERIOD(total_nodesm0)>=60, total_nodesm0/(PERIOD(total_nodesm0)/60), total_nodesm0)", "visible": false, "label": "${account_id},tsanghan-ce6", "accountId": "${account_id}" } ],
                    [ "ContainerInsights", "node_status_condition_ready", "ClusterName", "tsanghan-ce6", { "id": "ready_nodesm0", "stat": "Sum", "visible": false, "accountId": "${account_id}" } ],
                    [ ".", "cluster_node_count", ".", ".", { "id": "total_nodesm0", "label": "Sum", "stat": "Sum", "visible": false, "accountId": "${account_id}" } ]
                ],
                "liveData": false,
                "period": 60,
                "view": "singleValue"
            }
